{
    "name": "villermen/doctrine-identity-queuer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Villermen",
            "email": "villermen@gmail.com"
        }
    ],
    "require": {
        "doctrine/orm": "^2.6",
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": {
            "Villermen\\DoctrineIdentityQueuer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Villermen\\DoctrineIdentityQueuer\\Test\\": "test/"
        }
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^4.0"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    }
}
